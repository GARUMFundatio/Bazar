<div id="tabs-6content">
<%- flash[:notice] %>
<% form_for @user_session, :url => user_sessions_path do |f| %>
  
  <p><%= f.label :Usuario %>
  <%= f.text_field :login %></p>
  <p><%= f.label :Contraseña %>
  <%= f.password_field :password %></p>
  <p><%= f.check_box :remember_me %><%= f.label :Recuerdame %></p>
  <%= f.submit "Entrar" %> <%= link_to("He olvidado mi contraseña", new_password_reset_path) -%>
<% end %>
</div>
