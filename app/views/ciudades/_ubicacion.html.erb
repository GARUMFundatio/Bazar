<% if !ubicacion.ciudad.nil? %>
<tr>

	<td>
		
		<% ciudad = Ciudad.find(ubicacion.ciudad) %>
		
		<%= link_to(ciudad.descripcion, ciudades_path+'/'+ciudad.friendly_id) %>, 
		<%= link_to(ciudad.pais.descripcion, paises_path+'/'+ciudad.pais.friendly_id) %>
	
	</td>

	<td>
		<%= link_to(Bazarcms::Empresa.find(ubicacion.empresa_id).nombre, "/home/fichaempresa/#{BZ_param('BazarId')}/#{ubicacion.empresa_id}") %>
	</td>

</tr>
<% end %>